{
  "version": "0.0.0",
  "name": "CHANGE_ME",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint --ext js,jsx,ts,tsx .",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
    "lint:locales": "intl-equalizer",
    "test": "cd ./react && yarn test",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "intl-equalizer": {
    "referenceLocale": "en",
    "localeDirectory": "messages"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,js,tsx,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ],
    "messages/*.json": [
      "yarn lint:locales"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.4",
    "@babel/preset-react": "^7.22.3",
    "@babel/preset-typescript": "^7.21.5",
    "@storybook/addon-a11y": "^7.0.18",
    "@storybook/addon-essentials": "7.0.18",
    "@storybook/addon-interactions": "7.0.18",
    "@storybook/addon-links": "7.0.18",
    "@storybook/addon-storysource": "^7.0.18",
    "@storybook/blocks": "7.0.18",
    "@storybook/react": "8.6.2",
    "@storybook/react-webpack5": "7.0.18",
    "@storybook/testing-library": "0.0.14-next.2",
    "@vtex/danger": "^0.2.7",
    "@vtex/intl-equalizer": "^2.5.0",
    "@vtex/prettier-config": "^0.3.5",
    "eslint": "^7.14.0",
    "eslint-config-vtex": "^12.8.11",
    "eslint-config-vtex-react": "^6.8.3",
    "husky": "^8.0.3",
    "lint-staged": "^10.5.1",
    "prettier": "^2.2.0",
    "prop-types": "15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "storybook": "7.0.18",
    "storybook-addon-pseudo-states": "^2.0.1",
    "storybook-css-modules": "^1.0.8",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "vtex-tachyons": "^3.2.2"
  }
}
